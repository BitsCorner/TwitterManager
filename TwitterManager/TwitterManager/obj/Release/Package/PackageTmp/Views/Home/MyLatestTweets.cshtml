@using TwitterHelper.Entity
@model List<TwitterStatus>
@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>

@foreach (var item in Model)
{
    @item.text <br />
    @item.source <br />
    @item.created_at <hr />
}